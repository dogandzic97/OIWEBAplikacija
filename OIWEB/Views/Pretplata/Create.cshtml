@model OIWEB.Pretplata

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>


@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
<div class="form-horizontal">
    <h4>Pretplata</h4>
    <hr />
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    <div class="form-group">
        @Html.Label("Pretplatnicki broj:")
        <div class="col-md-10">
            <input type="text" name="PretplatnickiBroj" />
        </div>
    </div>

    <div class="form-group">
        @Html.Label("Naziv pravnog lica:")
        <div class="col-md-10">
            <input type="text" name="NaziPravnogLica" />
        </div>
    </div>

    <div class="form-group">
        @Html.Label("Broj tekuceg racuna")
        <div class="col-md-10">
            <input type="text" name="BrojTekucegRacuna" />
        </div>
    </div>

    <div class="form-group">
        @Html.Label("PIB")
        <div class="col-md-10">
            <input type="text" name="PIB" />
        </div>
    </div>

    <div class="form-group">
        @Html.Label("Mesto")
        <div class="col-md-10">
            <input type="text" name="PretplatnickiBroj" />
        </div>
    </div>

    <div class="form-group">
        @Html.Label("Postanski broj:")
        <div class="col-md-10">
            <input type="text" name="PostanskiBroj" />
        </div>
    </div>
    <div class="form-group">
        @Html.Label("Ulica i broj:")
        <div class="col-md-10">
            <input type="text" name="UlicaIBroj" />
        </div>
    </div>
    <div class="form-group">
        @Html.Label("Ime:")
        <div class="col-md-10">
            <input type="text" name="Ime" />
        </div>
    </div>
    <div class="form-group">
        @Html.Label("Prezime")
        <div class="col-md-10">
            <input type="text" name="Prezime" />
        </div>
    </div>
    <div class="form-group">
        @Html.Label("Telefon")
        <div class="col-md-10">
            <input type="text" name="Telefon" />
        </div>
    </div>
    <div class="form-group">
        @Html.Label("Faks")
        <div class="col-md-10">
            <input type="text" name="Faks" />
        </div>
    </div>
    <div class="form-group">
        @Html.Label("Email")
        <div class="col-md-10">
            <input type="email" name="Email " />
        </div>
    </div>
    <div class="form-group">
        @Html.Label("Pretplata na:")
        <div class="col-md-10">
            <input type="text" name="Trajanje" />
        </div>
    </div>

    <div class="form-group">
        @Html.Label("Kolicina")
        <div class="col-md-10">
            <input type="text" name="BrojPrimeraka" />
        </div>
    </div>
    <div class="form-group">
        @Html.Label("Nacin placanja")
        <div class="col-md-10">
            <input type="text" name="PretplatnickiBroj" />
        </div>
    </div>
    <div class="form-group">

        <input type="text" name="IDCasopis" value="@ViewBag.IDCasopis" />

    </div>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Kreiraj" class="btn btn-default" />
        </div>
    </div>
</div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
