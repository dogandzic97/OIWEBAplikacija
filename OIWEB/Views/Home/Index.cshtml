                        List<ClanciTxt> clanciTxt = (List<ClanciTxt>)ViewBag.Tekstovi;
                        using (Html.BeginForm("Delete", "Home", new { id = item.IDClanak }))
                        {

                            foreach (var tekst in clanciTxt)
                            {
                                if (item.IDClanak == tekst.IDClanak)
                                {



                                    <div class="row" data-aos="fade-up">

                                        <div class="col-md-7 pt-4">
                                            <h2>@item.Tip</h2>
